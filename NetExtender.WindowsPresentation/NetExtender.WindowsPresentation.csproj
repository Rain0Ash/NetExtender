<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
		<OutputType>Library</OutputType>
        <UseWpf>true</UseWpf>
		<LangVersion>10</LangVersion>
		<Authors>Rain0Ash</Authors>
        <PackageProjectUrl>https://github.com/Rain0Ash/NetExtender</PackageProjectUrl>
        <Version>0.8.7</Version>
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>../SignTool/Private/NetExtender.snk</AssemblyOriginatorKeyFile>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\NetExtender.Core\NetExtender.Core.csproj" />
    </ItemGroup>

    <ItemGroup>
      <Compile Update="Utilities\Alt.cs">
        <DependentUpon>KeyboardUtilities.cs</DependentUpon>
      </Compile>
      <Compile Update="Utilities\Ctrl.cs">
        <DependentUpon>KeyboardUtilities.cs</DependentUpon>
      </Compile>
      <Compile Update="Utilities\Shift.cs">
        <DependentUpon>KeyboardUtilities.cs</DependentUpon>
      </Compile>
      <Compile Update="Utilities\Win.cs">
        <DependentUpon>KeyboardUtilities.cs</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
