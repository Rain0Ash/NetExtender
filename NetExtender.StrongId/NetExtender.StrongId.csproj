        <EmbeddedResource Include="..\NetExtender.StrongId.Attributes\StrongId\Attributes\*.cs" Link="StrongId\Attributes\%(Filename)%(Extension)" />
        <EmbeddedResource Include="..\NetExtender.StrongId.Attributes\StrongId\Interfaces\*.cs" Link="StrongId\Interfaces\%(Filename)%(Extension)" />
